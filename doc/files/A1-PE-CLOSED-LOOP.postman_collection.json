      "name": "[CONFIGURE-STEP 1] Get datafile CONSUL key value",
      "event": [
        {
          "listen": "test",
          "script": {
            "exec": [
              "consul_datafile_dmaap_key__constans_str = \"datafile-collector:dmaap\"\r",
              "\r",
              "pm.response.json().forEach((key) => {\r",
              "    if (key.includes(consul_datafile_dmaap_key__constans_str)) {\r",
              "            pm.environment.set(\"consul-datafile-dmaap-key\", key);\r",
              "        }\r",
              "    }\r",
              ")"
            ],
            "type": "text/javascript"
          }
        }
